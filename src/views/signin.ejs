<body>

  <header class="header">
  
    <img class="logo2" src="/images/mklogo.png" alt="">

    <div class="textonombre">
      <h2>Bienvenido a Master Key</h2>
  </div>

  <nav class="navigation">
    <ul>
      <li><a href="/index"><i class="fa-solid fa-house-chimney" style="color: #ffffff;"></i>  Inicio </a></li>
      <li><a href="#seccion1"><i style="color: #ffffff;" class="fa-solid fa-building"></i> Nosotros </a></li>
      <li><a href="#seccion3"><i style="color: #ffffff;" class="fa-solid fa-microchip"></i> Servicios </a></li>
      <li><a href="/signin"><i  style="color: #ffffff;" class="fa-solid fa-right-to-bracket"></i></i>  iniciar sesión </a></li>
  </ul>

  </nav>
</header>


  <img class="imagen" src="/images/login2.png" alt="">

  <div class="login-box">
    <h1>Inicia Sesión</h1>
    <form action="/signin" method="POST">
        <div class="user-box">
          <i class="fa-solid fa-envelope" style="color: #ffffff; position: fixed; top: 110px; left: 20px;"></i>
          <input type="email" name="email">
            <label>Email</label>
        </div>
        <div class="user-box">
          <i class="fa-solid fa-lock" style="color: #ffffff; position: fixed; top: 180px; left: 20px;"></i>
          <input type="password" name="password"> 
            <label>Password</label>
        </div>
        
        <% if (signinMessage && signinMessage.length > 0) { %>
          <script>
              Swal.fire({
                  text: "<%= signinMessage %>",
                  icon: "error",
                  confirmButtonText: "Aceptar"
              });
          </script>
      <% } %>
    
      
        
        <a href="#">
          <span></span>
          <span></span>
          <span></span>
          <span></span>
          <button class="boton"  type="submit">Ingresar</button> 
      </a>
      </a>
      </a>


  </div>      

